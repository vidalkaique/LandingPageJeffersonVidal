# URL do seu projeto Supabase
# Exemplo: https://xyzabcdef.supabase.co
VITE_SUPABASE_URL=

# Chave pública (anon key) do Supabase
# Encontre em: Project Settings > API > anon/public
VITE_SUPABASE_ANON_KEY=

